<% report.items.each do |key, result| %>
  <% next unless result.any? %>
  <p><strong><%= key %></strong></p>
  <table>
    <thead>
      <% result.first.keys.each do |header| %>
        <th><%= header %></th>
      <% end %>
    </thead>
    <tbody>
      <% result.each do |row| %>
        <tr>
          <% row.values.each do |value| %>
            <td><%= value %></td>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>
